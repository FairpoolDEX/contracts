{
  "name": "shield-contracts",
  "version": "1.0.1",
  "description": "Shield Finance contracts",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "yarn clean && yarn compile",
    "clean": "FEES=20:1 hardhat clean --show-stack-traces",
    "compile": "FEES=20:1 hardhat compile --show-stack-traces",
    "typecheck": "tsc --noEmit",
    "fork": "FEES=20:1 hardhat node --fork https://eth-mainnet.alchemyapi.io/v2/$ALCHEMY_MAINNET_API_KEY",
    "lint": "yarn lint:code && yarn lint:markdown",
    "lint:code": "solhint --fix contracts/*.sol && eslint .",
    "lint:markdown": "markdownlint --fix --ignore node_modules **/*.md",
    "format": "prettier --check \"contracts/**/*.sol\" \"**/*.ts\" \"**/*.md\" \"**/*.yml\"",
    "test": "FEES=20:1 TS_NODE_TRANSPILE_ONLY=1 hardhat --show-stack-traces test",
    "test:watch": "nodemon -x 'FEES=20:1 TS_NODE_TRANSPILE_ONLY=1 hardhat --show-stack-traces test'",
    "coverage": "FEES=20:1 yarn build && FEES=20:1 hardhat coverage --temp artifacts",
    "flatten": "FEES=20:1 hardhat flatten contracts/BullToken.sol | grep -v SPDX > ./BullToken.flat.sol",
    "mythx:analyze": "mythx analyze --async --create-group --solc-version 0.8.4 --mode standard --include BullToken ./BullToken.flat.sol",
    "deploy:bull:bsctestnet": "FEES=20:1 hardhat deployContract --contract BullToken --upgradeable true --constructor-args-module tasks/arguments/BullToken.arguments.ts --network bsctestnet",
    "deploy:taur:bsctestnet": "FEES=20:1 hardhat deployContract --contract GenericToken --constructor-args-module tasks/arguments/MarnotaurToken.arguments.ts --network bsctestnet",
    "deploy:ple:avaxtestnet": "FEES=40:5 hardhat deployContract --contract GenericToken --constructor-args-module tasks/arguments/PlethoriToken.arguments.ts --network avaxtestnet",
    "rebrand:writeClaims:mainnet": "FEES=20:1 DEBUG=app:* hardhat --show-stack-traces writeClaims --network mainnet --expectations test/expectations/writeClaims.rebrand.ts --out ~/Downloads/2021-08-03/balances.json --cache-key rebrand:dummy",
    "rebrand:setClaims:bsctestnet": "FEES=20:1 DEBUG=app:* hardhat --show-stack-traces setClaims --network bsctestnet --claims ~/Downloads/2021-08-03/balances.json --cache-key rebrand:dummy",
    "rebrand:deployColiToken:bsctestnet": "FEES=20:1 DEBUG=app:* hardhat --show-stack-traces deployColiToken --network bsctestnet --from-network mainnet --allocations ~/shield-contracts-data/Shield\\ Frozen\\ Wallets\\ -\\ Wallets.csv --expectations test/expectations/deployColiToken.rebrand.ts --cache-key rebrand:dummy"
  },
  "engines": {
    "node": "^16.13.0"
  },
  "dependencies": {
    "@ethersproject/hardware-wallets": "^5.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.1",
    "@openzeppelin/contracts-upgradeable": "^4.3.1",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "@pancakeswap/sdk": "^2.4.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-fs-hash": "^0.0.1",
    "@types/luxon": "^1.27.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "cache-manager": "^3.6.0",
    "cache-manager-fs-hash": "^1.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "csv-parser": "^3.0.0",
    "debug": "^4.3.3",
    "decimal.js": "^10.3.1",
    "dotenv": "^8.6.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.4",
    "etherscan-api": "^10.0.5",
    "execa": "^5.0.1",
    "fast-check": "^2.17.0",
    "hardhat": "^2.8.1",
    "hardhat-deploy": "^0.9.24",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-watcher": "^2.1.1",
    "husky": "^7.0.4",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "longjohn": "^0.2.12",
    "luxon": "^2.0.1",
    "mkdirp": "^1.0.4",
    "neat-csv": "^6.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.11",
    "semver": "^7.3.5",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.14",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^6.1.0",
    "typescript": "~4.3.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.20",
    "@types/chai-as-promised": "^7.1.4",
    "@types/debug": "^4.1.7",
    "@types/find-up": "^2.1.1",
    "@types/lodash": "^4.14.171",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "hardhat-dependency-compiler": "^1.1.1",
    "markdownlint": "^0.22.0",
    "markdownlint-cli": "^0.26.0"
  }
}
