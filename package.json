{
  "name": "shield-contracts",
  "version": "1.0.0",
  "description": "Shield Finance contracts",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "GAS_PRICE=20 hardhat clean",
    "compile": "GAS_PRICE=20 hardhat compile",
    "lint": "solhint contracts/*.sol && eslint .",
    "lint:markdown": "markdownlint --ignore node_modules **/*.md",
    "format": "prettier --check \"contracts/**/*.sol\" \"**/*.ts\" \"**/*.md\" \"**/*.yml\"",
    "test": "GAS_PRICE=20 hardhat --show-stack-traces test",
    "test:watch": "nodemon -x 'GAS_PRICE=20 hardhat --show-stack-traces test'",
    "coverage": "GAS_PRICE=20 yarn build && GAS_PRICE=20 hardhat coverage --temp artifacts",
    "flatten": "GAS_PRICE=20 hardhat flatten contracts/BullToken.sol | grep -v SPDX > ./BullToken.flat.sol",
    "mythx:analyze": "mythx analyze --async --create-group --solc-version 0.8.4 --mode standard --include BullToken ./BullToken.flat.sol"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/luxon": "^1.27.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "csv-parser": "^3.0.0",
    "decimal.js": "^10.3.1",
    "dotenv": "^8.6.0",
    "eslint-plugin-react": "^7.24.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.2.0",
    "etherscan-api": "^10.0.5",
    "execa": "^5.0.1",
    "fast-check": "^2.17.0",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.7.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.1.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.1",
    "neat-csv": "^6.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.11",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.14",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^3.0.0",
    "typescript": "^4.3.2",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.20",
    "@types/chai-as-promised": "^7.1.4",
    "@types/lodash": "^4.14.171",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "markdownlint": "^0.22.0",
    "markdownlint-cli": "^0.26.0"
  }
}
