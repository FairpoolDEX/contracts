    "bignumber.js": "^9.1.1",
    "cache-manager": "^3.6.0",
    "cache-manager-fs-hash": "^1.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.1.0",
    "chai-subset": "^1.6.0",
